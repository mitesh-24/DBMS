create procedure calper4() 
begin 
declare id,sub1,sub2,sub3,tot int; 
declare per float(5,2);
declare i,no int(3);
SET i = 0;
SELECT count(*) into no from STUDENT;
WHILE i<= no DO 
select sid,S1,S2,S3 into id,sub1,sub2,sub3 from STUDENT LIMIT i,1; 
SET tot = sub1+sub2+sub3; 
SET per = (tot/3); 
SELECT id,tot,per;
SET i = i+1;
end WHILE; 
END
//
